{
  "name": "JustPick",
  "version": "1.0.0",
  "description": "Justpick",
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "client": "node scripts/start-client.js",
    "start": "node server.js",
    "start-local": "concurrently \"nodemon server.js\" \"npm run client\"",
    "start-debug": "concurrently \"nodemon --inspect server.js\" \"npm run client\"",
    "build": "node scripts/build.js",
    "jest": "jest --config=jest.config.json",
    "jest-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --config=jest.config.json",
    "testcafe": "testcafe chrome tests/",
    "testcafe-debug": "testcafe --inspect-brk chrome tests/",
    "test": "npm run jest && npm run testcafe"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "jest": "^22.4.3",
    "nodemon": "^1.11.0",
    "react-test-renderer": "^16.3.0",
    "supertest": "^3.0.0",
    "testcafe": "^0.19.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "moment": "^2.21.0",
    "mongoose": "^5.0.7",
    "morgan": "^1.10.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "raf": "^3.4.0",
    "react": "^16.3.0",
    "react-big-calendar": "^0.19.0",
    "react-dom": "^16.3.0",
    "react-dropzone": "^4.2.9",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-stars": "^2.2.5",
    "superagent": "^3.8.2",
    "validator": "^9.4.1"
  }
